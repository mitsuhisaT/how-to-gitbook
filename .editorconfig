# EditorConfig is awesome:
https://EditorConfig.org

# top-most EditorConfig filr
root = true

# Unix-style newline with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.md]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2
